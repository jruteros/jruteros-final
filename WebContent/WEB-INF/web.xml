<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>JyAA-Entrega1</display-name>
   <welcome-file-list>
    <welcome-file>index.xhtml</welcome-file>
    <!-- <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file> -->
  </welcome-file-list> 
  <servlet>
    <description></description>
    <display-name>Login</display-name>
    <servlet-name>Login</servlet-name>
    <servlet-class>misServlets.Login</servlet-class>
    <init-param>
      <param-name>nombreAdmin</param-name>
      <param-value>admin</param-value>
    </init-param>
    <init-param>
      <param-name>claveAdmin</param-name>
      <param-value>admin</param-value>
    </init-param>
    <init-param>
      <param-name>nombreUsuario</param-name>
      <param-value>usuario</param-value>
    </init-param>
    <init-param>
      <param-name>claveUsuario</param-name>
      <param-value>usuario</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/Login</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>PruebaPersistencia</display-name>
    <servlet-name>PruebaPersistencia</servlet-name>
    <servlet-class>pruebas.PruebaPersistencia</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PruebaPersistencia</servlet-name>
    <url-pattern>/PruebaPersistencia</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>PruebaDAOS</display-name>
    <servlet-name>PruebaDAOS</servlet-name>
    <servlet-class>pruebas.PruebaDAOS</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PruebaDAOS</servlet-name>
    <url-pattern>/PruebaDAOS</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Faces Servlet</servlet-name>
    <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>Faces Servlet</servlet-name>
<!--<url-pattern>/faces/*</url-pattern> -->
 <url-pattern>*.xhtml</url-pattern>
  </servlet-mapping>
  <context-param>
    <description>State saving method: 'client' or 'server' (=default). See JSF Specification 2.5.2</description>
    <param-name>javax.faces.STATE_SAVING_METHOD</param-name>
    <param-value>client</param-value>
  </context-param>
  <context-param>
    <param-name>javax.servlet.jsp.jstl.fmt.localizationContext</param-name>
    <param-value>resources.application</param-value>
  </context-param>
  <listener>
    <listener-class>com.sun.faces.config.ConfigureListener</listener-class>
  </listener>
</web-app>