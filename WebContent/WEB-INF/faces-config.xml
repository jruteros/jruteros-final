<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">

	<navigation-rule> 
		<from-view-id>/login.xhtml</from-view-id> 
		<navigation-case> 
			<from-outcome>LoginUsuario</from-outcome> 
			<to-view-id>/misrutas.xhtml</to-view-id> 
			<redirect></redirect>
		</navigation-case> 
		<navigation-case> 
			<from-outcome>LoginAdministrador</from-outcome> 
			<to-view-id>/administradorAdminUsuarios.xhtml</to-view-id>
			<redirect></redirect> 
		</navigation-case>
		<navigation-case> 
			<from-outcome></from-outcome> 
			<to-view-id>/login.xhtml</to-view-id> 
		</navigation-case>  
	</navigation-rule>

	<navigation-rule> 
		<from-view-id>*</from-view-id> 
		<navigation-case> 
			<from-outcome>Logout</from-outcome> 
			<to-view-id>/index.xhtml</to-view-id> 
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule> 
		<from-view-id>/administradorAdminUsuarios.xhtml</from-view-id> 
		<navigation-case> 
			<from-outcome>verMas</from-outcome> 
			<to-view-id>/administradorVerMas.xhtml</to-view-id> 
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	
	<managed-bean> 
		<description>Login Bean</description> 
		<managed-bean-name>LoginBean</managed-bean-name> 
		<managed-bean-class>beans.LoginBean</managed-bean-class> 
		<managed-bean-scope>view</managed-bean-scope> 
	</managed-bean>
	<managed-bean> 
		<description>Usuario Bean</description> 
		<managed-bean-name>UsuarioBean</managed-bean-name> 
		<managed-bean-class>beans.UsuarioBean</managed-bean-class> 
		<managed-bean-scope>request</managed-bean-scope> 
	</managed-bean>
	<managed-bean> 
		<description>Registrar Bean</description> 
		<managed-bean-name>RegistrarBean</managed-bean-name> 
		<managed-bean-class>beans.RegistrarBean</managed-bean-class> 
		<managed-bean-scope>request</managed-bean-scope> 
	</managed-bean>
	<managed-bean> 
	<description>Recuperar Usuario Bean</description> 
		<managed-bean-name>RecuperarUsuarioBean</managed-bean-name> 
		<managed-bean-class>beans.RecuperarUsuarioBean</managed-bean-class> 
		<managed-bean-scope>view</managed-bean-scope> 
	</managed-bean>
</faces-config>
